<div class="jumbotron">
  <h1>Your Profile</h1>
  <p>
    Update your profile information!
  </p>
</div>

<div class="row">
  <div class="col-sm-4">
  </div>
  <form method="post" action="edit">
    <div class="col-sm-4">
        <table class="table table-bordered">
          <tbody>
            <tr>
              <td>Google Name</td>
              <td><%= user.googlename %></td>
            </tr>
            <tr>
              <td>Google Email</td>
              <td><%= user.googleemail %></td>
            </tr>
            <tr>
              <td>Preferred Name</td>
              <td>
                <input type="text" name="username"
                   value="<%= user.username %>">
              </td>
            </tr>
            <tr>
              <td>Profile Image URL</td>
              <td>
                <input type="text" name="imageURL"
                   value="<%= user.imageURL %>">
              </td>
            </tr>
          </tbody>
        </table>
    </div>

  <div class="col-sm-4">
  </div>
</div>
<input class="btn btn-lg btn-success" type="submit" value="Save Profile">
</form>
